# ==============================================================================
# Environment Configuration
# ==============================================================================
# Copy this file to .env and fill in your actual API keys
# DO NOT commit .env to version control!

# ==============================================================================
# LLM Configuration
# ==============================================================================

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here
# OPENAI_BASE_URL=https://api.openai.com/v1  # Optional: custom endpoint
OPENAI_MODEL=gpt-4o

# Anthropic (Claude) Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# DeepSeek Configuration
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Aliyun DashScope Configuration
DASHSCOPE_API_KEY=your_dashscope_api_key_here

# ==============================================================================
# Tool-Specific API Keys (REST APIs)
# ==============================================================================

# RapidAPI Configuration (for weather, traffic, and other RapidAPI endpoints) examples
RAPIDAPI_KEY=your_rapidapi_key_here

# Amap (高德地图) Configuration
AMAP_API_KEY=your_amap_api_key_here

# Weather API Configuration
WEATHER_API_KEY=your_weather_api_key_here

# Traffic Data API Configuration
TRAFFIC_API_KEY=your_traffic_api_key_here

